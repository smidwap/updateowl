- content_for :aside do
  = render "overview", student: @student

- content_for :content do
  %section.messages
    %header
      %h3 #{@student.full_name}'s Archive
      = render "shared/student_edit_nav", student: @student
    - if content_for?(:filter_alert)
      .filter_alert= yield(:filter_alert)
    = yield

= render template: "layouts/application"